{
  "uid": "fed-ai-overview",
  "title": "fed_AI Overview",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "type": "timeseries",
      "title": "Router Inference Requests",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(rate(router_inference_requests_total[5m]))",
          "legendFormat": "requests/sec"
        }
      ],
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 }
    },
    {
      "type": "timeseries",
      "title": "Router Inference Latency (p95)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(router_inference_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95"
        }
      ],
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 8 }
